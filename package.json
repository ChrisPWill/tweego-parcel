{
  "name": "twine-tweego-parcel-boilerplate",
  "version": "1.0.0",
  "license": "MIT",
  "devDependencies": {
    "@types/twine-sugarcube": "^2.36.11",
    "parcel": "^2.12.0",
    "typescript": "^5.5.3"
  },
  "scripts": {
    "build:parcel": "parcel build src/index.ts",
    "build:site": "tweego src/index.twee -o dist/index.html && cp -r images/ dist",
    "build": "yarn build:parcel && yarn build:site",
    "start": "yarn build && xdg-open dist/index.html"
  }
}
